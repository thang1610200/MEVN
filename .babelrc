{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        ["module-resolver",{
            "root":["./"],
            "alias":{
                "@":"./",
                "@pages":"./client/pages",
                "@components":"./client/components",
                "@controllers":"./server/controllers",
                "@config":"./server/config/default",
                "@routes":"./server/routes"
            }
        }]
    ]
}